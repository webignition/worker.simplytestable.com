	<script type="text/javascript">
		var Page = (function() {	
			var $grid = $( '.verticalSliderOuter' );	
			return {
				init : function() {
					$grid.find( 'div.bb-bookblock' ).each( function( i ) {
											
						var $bookBlock = $( this ),
													
							bb = $bookBlock.bookblock( {
								orientation : 'horizontal',
								speed : 400
							} );
							
							var $slides = $bookBlock.children();
							
							$(this).mouseenter('click touchstart', function() {
								$bookBlock.bookblock( 'last' );
								$('a', $bookBlock).fadeIn(500);
								return false;
							});
			
							$(this).mouseleave('click touchstart', function() {
								$bookBlock.bookblock( 'first' );
								return false;
							});
												
					});
				}
			};
		})();
		
		$('.boxLeft').css({'margin-top':'-100px'});
		
		$('.verticalSliderOuter').jScrollPane({
			showArrows: true
		});
		
		/*jQuery('#mycarousel').jcarousel({
			vertical: true,
			scroll: 1,
		});*/
		
		//bx slider
		$('#boxSlider').bxSlider({
			slideWidth: 300,
			minSlides: 2,
			maxSlides: 3,
			moveSlides: 1,
			slideMargin: 30,
			auto: true
		  });
	
		Page.init();
</script>


