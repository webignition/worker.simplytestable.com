{
    "name": "webignition/worker.simplytestable.com",
    "description": "Source for workers to which tasks for http://app.simplytestable.com are assigned",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "require": {
        "php": ">=5.5.0",
        "symfony/symfony": "2.3.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "1.2.*",
        "twig/extensions": "1.0.*",
        "symfony/assetic-bundle": "2.3.*",
        "symfony/monolog-bundle": "2.3.*",
        "sensio/distribution-bundle": "2.3.*",
        "sensio/framework-extra-bundle": "2.3.*",
        "sensio/generator-bundle": "2.3.*",
        "incenteev/composer-parameter-handler": "~2.0",
        "jms/security-extra-bundle": "1.5.*",
        "jms/di-extra-bundle": "1.4.*",
        "doctrine/migrations": "~1.5",
        "doctrine/doctrine-migrations-bundle": "~1.2",
        "jms/serializer-bundle": "~2.0",
        "webignition/url": ">=1.0,<2.0",
        "webignition/internet-media-type":">=0.4.4,<2.0",
        "querypath/QueryPath": "3.0.2",
        "webignition/web-page-model":">=1.1,<2",
        "webignition/web-json-model":">=1.1,<2",
        "webignition/absolute-url-deriver":">=1.4",
        "winzou/console-bundle":"1.0.0",
        "guzzlehttp/guzzle":">=5.2.0,<5.3",
        "doctrine/doctrine-fixtures-bundle": "~2.3",
        "webignition/html-document-link-finder":">=1.2,<2.0",
        "webignition/html-document-link-checker":">=1.1.2,<2.0",
        "webignition/html-validator-wrapper":">=0.4.2",
        "webignition/html-validator-output-parser":">=0.7,<1.0",
        "webignition/css-validator-wrapper":">=1.0,<2.0",
        "webignition/web-resource-service":">=1.1,<2.0",
        "webignition/html-document-type-extractor":">=0.1.1,<1.0",
        "webignition/html-document-type-validator":">=0.1,<1.0",
        "webignition/node-jslint-wrapper":">=0.4,<1",
        "webignition/cookie-url-matcher":">=0.1,<1",
        "chrisboulton/php-resque": "dev-master",
        "chrisboulton/php-resque-scheduler": "dev-master",
        "bcc/resque-bundle": "dev-master",
        "guzzlehttp/retry-subscriber":">=2.0.2,<3",
        "guzzlehttp/cache-subscriber": "dev-master",
        "webignition/guzzle-curl-exception":">=0.1.2,<1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "3.*",
        "mockery/mockery": "~0.9"
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.3-dev"
        }
    }
}
