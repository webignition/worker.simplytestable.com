{
    "name": "webignition/worker.simplytestable.com",
    "description": "Source for workers to which tasks for http://app.simplytestable.com are assigned",
    "autoload": {
        "psr-4": {
            "SimplyTestable\\WorkerBundle\\": "src/SimplyTestable/WorkerBundle"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "sensio/distribution-bundle": "^5.0.19",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/symfony": "3.3.*",
        "twig/twig": "^1.0||^2.0",
        "php": ">=5.6.0",
        "doctrine/migrations": "~1.5",
        "doctrine/doctrine-migrations-bundle": "~1.2",
        "webignition/url": ">=1.0,<2.0",
        "webignition/internet-media-type":">=0.4.4,<2.0",
        "querypath/QueryPath": "3.0.2",
        "webignition/web-page-model":">=1.3,<2",
        "webignition/web-json-model":">=1.1.6,<2",
        "webignition/absolute-url-deriver":">=1.4",
        "guzzlehttp/guzzle":"~5",
        "doctrine/doctrine-fixtures-bundle": "~2.3",
        "webignition/html-document-link-finder":">=1.4.1,<2.0",
        "webignition/html-document-link-checker":">=1.2.3,<2.0",
        "webignition/html-validator-wrapper":">=0.5",
        "webignition/html-validator-output-parser":">=0.7,<1.0",
        "webignition/css-validator-wrapper":">=1.3.1,<2.0",
        "webignition/web-resource-service":">=1.4.1,<2.0",
        "webignition/html-document-type-extractor":">=0.1.1,<1.0",
        "webignition/html-document-type-validator":">=0.1,<1.0",
        "webignition/node-jslint-wrapper":">=0.5.2,<1",
        "webignition/cookie-url-matcher":">=0.1,<1",
        "chrisboulton/php-resque": "dev-master",
        "chrisboulton/php-resque-scheduler": "dev-master",
        "resquebundle/resque": "1.2.0",
        "guzzlehttp/retry-subscriber":">=2.0.2,<3",
        "guzzlehttp/cache-subscriber": "dev-master",
        "webignition/guzzle-curl-exception":">=0.1.4,<1",
        "webignition/resque-job-factory": ">=0.1.1,<1.0"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "phpunit/phpunit": "~5.0",
        "squizlabs/php_codesniffer": "3.*",
        "mockery/mockery": "~0.9",
        "php-mock/php-mock-mockery": "1.1.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "test-unit": "./vendor/bin/phpunit --colors tests/WorkerBundle/Unit",
        "test-functional": "./vendor/bin/phpunit --colors tests/WorkerBundle/Functional/",
        "test": [
            "@composer test-unit",
            "@composer test-functional"
        ],
        "cs": "./vendor/bin/phpcs --colors --standard=PSR2 --extensions=php src/SimplyTestable/WorkerBundle tests/",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.7-dev"
        }
    }
}
