{
    "name": "webignition/worker.simplytestable.com",
    "description": "Source for workers to which tasks for http://app.simplytestable.com are assigned",
    "autoload": {
        "psr-4": {
            "SimplyTestable\\WorkerBundle\\": "src/SimplyTestable/WorkerBundle"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=5.6.0",
        "doctrine/doctrine-bundle": "~1.4",
        "doctrine/orm": "^2.4.8",
        "incenteev/composer-parameter-handler": "~2.0",
        "sensio/distribution-bundle": "~4.0",
        "sensio/framework-extra-bundle": "^3.0.2",
        "symfony/monolog-bundle": "^3.0.2",
        "symfony/swiftmailer-bundle": "~2.3,>=2.3.10",
        "symfony/symfony": "2.8.*",
        "twig/twig": "^1.0||^2.0",
        "jms/security-extra-bundle": "1.6.*",
        "jms/di-extra-bundle": "1.4.*",
        "doctrine/migrations": "~1.5",
        "doctrine/doctrine-migrations-bundle": "~1.2",
        "webignition/url": ">=1.0,<2.0",
        "webignition/internet-media-type":">=0.4.4,<2.0",
        "querypath/QueryPath": "3.0.2",
        "webignition/web-page-model":">=1.1,<2",
        "webignition/web-json-model":">=1.1,<2",
        "webignition/absolute-url-deriver":">=1.4",
        "guzzlehttp/guzzle":">=5.2.0,<5.3",
        "doctrine/doctrine-fixtures-bundle": "~2.3",
        "webignition/html-document-link-finder":">=1.4.1,<2.0",
        "webignition/html-document-link-checker":">=1.2.2,<2.0",
        "webignition/html-validator-wrapper":">=0.5",
        "webignition/html-validator-output-parser":">=0.7,<1.0",
        "webignition/css-validator-wrapper":">=1.3,<2.0",
        "webignition/web-resource-service":">=1.2.1,<2.0",
        "webignition/html-document-type-extractor":">=0.1.1,<1.0",
        "webignition/html-document-type-validator":">=0.1,<1.0",
        "webignition/node-jslint-wrapper":"dev-master",
        "webignition/cookie-url-matcher":">=0.1,<1",
        "chrisboulton/php-resque": "dev-master",
        "chrisboulton/php-resque-scheduler": "dev-master",
        "bcc/resque-bundle": "dev-master",
        "guzzlehttp/retry-subscriber":">=2.0.2,<3",
        "guzzlehttp/cache-subscriber": "dev-master",
        "webignition/guzzle-curl-exception":">=0.1.2,<1"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "symfony/phpunit-bridge": "~2.7",
        "phpunit/phpunit": "~5.0",
        "squizlabs/php_codesniffer": "3.*",
        "mockery/mockery": "~0.9",
        "php-mock/php-mock-mockery": "1.1.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "test-unit": "./bin/phpunit -c app --colors src/SimplyTestable/WorkerBundle/Tests/Unit/",
        "test-functional": "./bin/phpunit -c app --colors src/SimplyTestable/WorkerBundle/Tests/Functional/",
        "test": [
            "@composer test-unit",
            "@composer test-functional"
        ],
        "cs": "./bin/phpcs --colors --standard=PSR2 --extensions=php src/SimplyTestable/WorkerBundle",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.7-dev"
        }
    }
}
