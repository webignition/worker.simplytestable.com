{
    "name": "webignition/worker.simplytestable.com",
    "description": "Source for workers to which tasks for http://app.simplytestable.com are assigned",
    "autoload": {
        "psr-4": {
            "SimplyTestable\\AppBundle\\": "src/SimplyTestable/AppBundle"
        },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=7.2.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.6",
        "sensio/framework-extra-bundle": "^5.0.0",
        "symfony/monolog-bundle": "^3.1.0",
        "symfony/symfony": "^4.0.0",
        "twig/twig": "^2.0",
        "doctrine/migrations": "~1.8",
        "doctrine/doctrine-migrations-bundle": "~1.2",
        "webignition/url": ">=2,<3",
        "webignition/internet-media-type":">=1,<2.0",
        "webignition/web-page-model":">=3,<4",
        "webignition/web-json-model":">=3,<4",
        "webignition/absolute-url-deriver":">=1.5",
        "guzzlehttp/guzzle":"^6",
        "doctrine/doctrine-fixtures-bundle": "~3",
        "webignition/html-document-link-finder":">=3.1,<4",
        "webignition/html-document-link-checker":">=2,<3",
        "webignition/html-validator-wrapper":">=0.5.1,<1",
        "webignition/html-validator-output-parser":">=0.7.1,<1",
        "webignition/css-validator-wrapper":">=2.2,<3",
        "webignition/web-resource-retriever":">=0.7,<1",
        "webignition/node-jslint-wrapper":">=1,<2",
        "chrisboulton/php-resque": "dev-master",
        "chrisboulton/php-resque-scheduler": "dev-master",
        "resquebundle/resque": "2.0.1",
        "kevinrob/guzzle-cache-middleware": "^3",
        "webignition/guzzle-curl-exception":">=1,<2",
        "webignition/html-document-type":">=0.1.1,<1.0",
        "webignition/http-history-container": ">=0.3,<1",
        "webignition/css-validator-output-parser": ">=3.2,<4",
        "webignition/guzzle-http-authentication-middleware": ">=0.1,<1",
        "webignition/guzzle-request-headers-middleware": ">=0.1,<1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0.0",
        "phpunit/phpunit": "~7.0",
        "squizlabs/php_codesniffer": "3.*",
        "mockery/mockery": "^1",
        "php-mock/php-mock-mockery": "^1"
    },
    "scripts": {
        "symfony-scripts": [
            "./bin/console cache:clear --no-warmup",
            "./bin/console cache:warmup"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ],
        "test-unit": "./vendor/bin/phpunit --colors tests/AppBundle/Unit",
        "test-functional": "./vendor/bin/phpunit --colors tests/AppBundle/Functional/",
        "test": [
            "@composer test-unit",
            "@composer test-functional"
        ],
        "cs": "./vendor/bin/phpcs --colors --standard=PSR2 --extensions=php src/SimplyTestable/AppBundle tests/",
        "ci": [
            "@composer cs",
            "@composer test"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "symfony-var-dir": "var",
        "symfony-bin-dir": "bin",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
